{% extends 'base.html.twig' %}

{% block title %}Analyse{% endblock %}

{% block body %}
    <h1>Analyse</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ analyse.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ analyse.type }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ analyse.statut }}</td>
            </tr>
            <tr>
                <th>Daterealisation</th>
                <td>{{ analyse.daterealisation ? analyse.daterealisation|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_analyse_index') }}">back to list</a>

    <a href="{{ path('app_analyse_edit', {'id': analyse.id}) }}">edit</a>

    {{ include('analyse/_delete_form.html.twig') }}
{% endblock %}
